<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
    <metadata>
        <id>securepointsslvpn</id>
        <title>Securepoint SSL VPN Client</title>
        <version>2.0.21</version>
        <authors>Securepoint Security Solutions</authors>
        <owners>wget</owners>
        <summary>SSL VPN Client for Windows (OpenVPN)</summary>
        <description>OpenVPN GUI for Windows.</description>
        <projectUrl>https://www.securepoint.de/en/products/nextgen-utm-firewalls/vpn-client.html</projectUrl>
        <tags>securepoint openvpn community tunnel ssl admin</tags>
        <copyright>2017 Securepoint GmbH</copyright>
        <licenseUrl>https://github.com/Securepoint/openvpn-client/blob/master/LICENSE.txt</licenseUrl>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <!-- Source: https://github.com/Securepoint/openvpn-client/blob/master/client/data/images_new/app-icon.png -->
        <iconUrl>https://cdn.rawgit.com/Securepoint/openvpn-client/master/client/data/images_new/app-icon.png</iconUrl>
        <dependencies>
            <!-- The id is the lowest version required to make the package work.
