<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>securepointsslvpn</id>
    <title>Securepoint SSL VPN Client</title>
    <version>2.0.40</version>
    <authors>Securepoint Security Solutions</authors>
    <owners>wget</owners>
    <summary>SSL VPN Client for Windows (OpenVPN)</summary>
    <description><![CDATA[
This repository contains the sources of the [package Securepoint SSL VPN](https://chocolatey.org/packages/securepointsslvpn) for [Chocolatey, the package manager for Windows](https://chocolatey.org/).

[For more information, please refer to the instructions available in the parent repository](https://github.com/wget/chocolatey-packages).
]]></description>
    <projectUrl>https://www.securepoint.de/en/products/nextgen-utm-firewalls/vpn-client.html</projectUrl>
    <tags>securepoint openvpn community tunnel ssl admin</tags>
    <copyright>2017 Securepoint GmbH</copyright>
    <licenseUrl>https://github.com/Securepoint/openvpn-client/blob/master/LICENSE.txt</licenseUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <!-- Source: https://github.com/Securepoint/openvpn-client/blob/master/client/data/images_new/app-icon.png -->
    <iconUrl>https://cdn.rawgit.com/Securepoint/openvpn-client/master/client/data/images_new/app-icon.png</iconUrl>
    <dependencies>
      <!-- The id is the lowest version required to make the package work.
         If we do not specify one, the chocolatey web validator will
         report a warning to be fixed in order to respect guidelines. -->
      <dependency id="chocolatey" version="0.9.10" />
    </dependencies>
    <releaseNotes>https://github.com/Securepoint/openvpn-client/releases</releaseNotes>
    <!-- Specific to Chocolatey -->
    <projectSourceUrl>https://github.com/Securepoint/openvpn-client</projectSourceUrl>
    <docsUrl>https://sourceforge.net/p/securepoint/wiki/Home/</docsUrl>
    <bugTrackerUrl>https://github.com/Securepoint/openvpn-client/issues</bugTrackerUrl>
    <packageSourceUrl>https://github.com/wget/chocolatey-package-securepointsslvpn</packageSourceUrl>
  </metadata>
  <files>
    <file src="tools\**" target="tools" exclude="**\utils\.git;**\utils\README.*" />
    <file src="legal\**" target="legal" />
  </files>
</package>
