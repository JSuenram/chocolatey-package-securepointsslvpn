                 report a warning to be fixed in order to respect guidelines. -->
            <dependency id="chocolatey" version="0.9.10" />
            <dependency id="autoit.commandline" version="3.3.14.2" />
        </dependencies>
        <releaseNotes>https://github.com/Securepoint/openvpn-client/releases</releaseNotes>  
        <!-- Specific to Chocolatey -->
        <projectSourceUrl>https://github.com/Securepoint/openvpn-client</projectSourceUrl>
        <docsUrl>https://sourceforge.net/p/securepoint/wiki/Home/</docsUrl>
        <bugTrackerUrl>https://github.com/Securepoint/openvpn-client/issues</bugTrackerUrl>
        <packageSourceUrl>https://github.com/wget/chocolatey-package-securepointsslvpn</packageSourceUrl>
  </metadata>
  <files>
      <file src="tools\**" target="tools" />
  </files>
</package>
